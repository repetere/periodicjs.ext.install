	<script type="text/javascript">
	//ribbonNotification.showRibbon( 'test flash message 5',4000,'error');
	//silkscreenModal.showSilkscreen("title","message",14,"default");
	window.addEventListener("load",function(e){
		<%
			var delayMultiplier=0;
			for(x in flash_messages){
				if(delayMultiplier === 0){
					%>
					ribbonNotification.showRibbon( '<%=flash_messages[x].join(". ") %>',4000,'<%= x %>');
						
					<%
				}
				delayMultiplier++;
			};
		%>
	},false);
	</script>
